{
  "schemaVersion": 1,
  "id": "createsifter",
  "version": "${version}",
  "name": "Create Sifter",
  "description": "A simple sifter for the amazing create mod",
  "authors": [
    "Shulej"
  ],
  "license": "CC0",
  "icon": "assets/createsifter/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.shulej.createsifter.CreateSifter"
    ],
    "client": [
      "io.github.shulej.createsifter.CreateSifterClient"
    ],
    "fabric-datagen": [
      "io.github.shulej.createsifter.ModDataGen"
    ],
    "jei_mod_plugin": [
      "io.github.shulej.createsifter.compat.jei.CreateSifterJEI"
    ]
  },
  "mixins": [
    "createsifter.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_api_version}",
    "create": ">=${create_version}",
    "minecraft": "${minecraft_version}"
  }
}
